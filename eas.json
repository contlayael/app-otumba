{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "ENV": "preview"
      }
    },
   "production": {
  "autoIncrement": true,
  "distribution": "store",
  "android": {
    "buildType": "apk"       
  },
  "env": {
    "ENV": "production"
  }
}

  },
  "submit": {
    "production": {}
  }
}
